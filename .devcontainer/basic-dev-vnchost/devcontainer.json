
{
    "name": "ubiquitous_bash-codespace-basic-dev-vnchost",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {}
    },
    "postCreateCommand": "wget 'https://raw.githubusercontent.com/mirage335/ubiquitous_bash/master/ubiquitous_bash.sh' ; ./ubiquitous_bash.sh _setupUbiquitous ; ./ubiquitous_bash.sh _vnchost-setup",
    "forwardPorts": [51001, 51002],
    "customizations": {
        "codespaces": {
          "repositories": {
            "my_org/my_repo": {
              "permissions": {
                "actions": "none",
                "checks": "none",
                "contents": "read",
                "issues": "none",
                "packages": "none",
                "pull_requests": "none",
                "repository_projects": "none",
                "security_events": "none",
                "statuses": "none",
                "metadata": "read"
              }
            }
          }
        }
      }
}


